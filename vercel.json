{
    "version": 2,
    "builds": [
        {
            "src": "django_project/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
                "src": "/static/(.*)",
                "dest": "/django_project/$1"
            },
            {
                "src": "/(.*)",
                "dest": "django_project/wsgi.py"
        }
    ]
}